

<%= form_for([@opp, @note]) do |f|%>
<p>
<%= f.label :title %>
<%= f.text_field :title %>
</p>

<p>
<%= f.label :description %> <br/>
<%= f.text_area :description, size: 120, autofocus: true %>
</p>
<p>
<%= f.label :date %>
<%= f.date_select(:date, :order =>[:month, :day, :year]) %>
</p>

<%= f.submit %>
	<% end %>
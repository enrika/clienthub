

<%= form_for(@opp) do |f| %>



<p>
<%= f.label :name %>
<%= f.text_field :name %>
</p>

<p>
<%= f.label 'Owner' %>
<%= f.text_field :prospect_name %>
</p>

<p>
<%= f.label :email %>
<%= f.text_field :email %>
</p>

<p>
<%= f.label :phone %>
<%= f.text_field :phone %>
</p>



<p>
<%= f.label :image %>
<%= f.text_field :image %>
</p>



<p>
<%= f.label :company %>
<%= f.text_field :company %>
</p>


<%= f.submit %>
<% end %>